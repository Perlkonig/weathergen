{
    "name": "weathergen",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "set NODE_ENV=development && vite",
        "build": "npx rimraf dist && set NODE_ENV=production && vite build --base=/weather",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "format": "prettier --write --plugin prettier-plugin-svelte ."
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^2.4.2",
        "@tsconfig/svelte": "^5.0.0",
        "prettier": "^3.0.3",
        "prettier-plugin-svelte": "^3.0.3",
        "rimraf": "^5.0.5",
        "svelte": "^4.0.5",
        "svelte-check": "^3.4.6",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.0",
        "typescript": "^5.0.2",
        "vite": "^4.4.5"
    },
    "dependencies": {
        "@event-calendar/core": "^2.3.3",
        "@event-calendar/day-grid": "^2.3.3",
        "@event-calendar/interaction": "^2.3.3",
        "@event-calendar/list": "^2.3.3",
        "@event-calendar/time-grid": "^2.3.3",
        "date-picker-svelte": "^2.8.0",
        "nanoid": "^5.0.2",
        "prando": "^6.0.1",
        "svelte-markdown": "^0.4.0"
    }
}
